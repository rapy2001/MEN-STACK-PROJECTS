<div>
    {{#if users}}
        {{#each users}}
            <div>
                <h1>rank: {{rank}} views: {{totalViews}}</h1>
                <div>
                    <img src = {{image}} alt = "error"/>
                    <div>
                        <h2> <a href = "/user/{{id}}/public">{{username}}</a></h2>
                        <p>{{description}}</p>
                    </div>
                </div>
            </div>
        {{/each}}
    {{else}}
        <div>
            <h1>No Users Yet ....</h1>
        </div>
    {{/if}}
</div>